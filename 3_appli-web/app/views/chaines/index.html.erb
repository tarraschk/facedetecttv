<p id="notice"><%= notice %></p>

<h1>Chaines</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Parser</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @chaines.each do |chaine| %>
      <tr>
        <td><%= chaine.name %></td>
        <td><%= chaine.parser %></td>
        <td><%= link_to 'Show', chaine %></td>
        <td><%= link_to 'Edit', edit_chaine_path(chaine) %></td>
        <td><%= link_to 'Destroy', chaine, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Chaine', new_chaine_path %>
